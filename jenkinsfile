pipeline {
    agent any
    stages{
        stage('Build'){
            steps {
                sh '/home/ansible/apache-maven-3.5.4/bin/mvn clean package'
            }
            post {
                success {
                    echo 'Now Archiving...'
                    archiveArtifacts artifacts: '**/*.war'
                }
            }
        }
    }
}
